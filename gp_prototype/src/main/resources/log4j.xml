    <!--<level value="info"/>-->
    <!--</logger>-->

    <!--<logger name="hubert.Main">-->
    <!--<level value="debug"/>-->
    <!--</logger>-->

    <logger name="hubert.evolution.fitness.TimeDifferentialQuotientFitnessFunction">
        <level value="info"/>
    </logger>

    <root>
        <level value="info"/>
        <appender-ref ref="CA"/>
    </root>
</log4j:configuration>